{
  "kind": "collectionType",
  "collectionName": "service_pages",
  "info": {
    "singularName": "service-page",
    "pluralName": "service-pages",
    "displayName": "Services of Elite Center",
    "description": "جميع الخدمات المقدمة في العيادة البيطرية"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "badge": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "isActive": {
      "type": "boolean",
      "default": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "order": {
      "type": "integer",
      "default": 0,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "features": {
      "type": "component",
      "repeatable": true,
      "component": "shared.service-feature",
      "min": 1,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "icons": {
      "type": "component",
      "repeatable": true,
      "component": "shared.service-icon",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    }
  }
}
